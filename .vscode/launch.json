{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "node",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      // "runtimeVersion": "14.5.0",
      "runtimeArgs": [
        "--inspect",
        "--experimental-json-modules",
        "--experimental-import-meta-resolve",
        "--trace-warnings",
        "--conditions=development"
        // "--preserve-symlinks",
        // "--preserve-symlinks-main"
      ],
      "console": "integratedTerminal",
      // "args": ["--coverage"],
      "autoAttachChildProcesses": true,
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": [
        // force-array-multiline
        // "node_modules/**",
        "<node_internals>/**"
      ]
    }
  ]
}
